{
  "name": "superfine",
  "description": "Minimal view layer for creating declarative web user interfaces.",
  "version": "6.0.1",
  "main": "dist/superfine.es5.js",
  "license": "MIT",
  "repository": "jorgebucaran/superfine",
  "types": "superfine.d.ts",
  "files": [
    "src",
    "dist",
    "superfine.d.ts"
  ],
  "author": "Jorge Bucaran",
  "keywords": [
    "superfine",
    "frontend",
    "virtual dom",
    "vdom"
  ],
  "scripts": {
    "test": "jest --coverage --no-cache",
    "build": "npm run build:clean && npm run build:gcc:es5 && npm run build:gcc:es6 && npm run build:gzip",
    "build:clean": "rm -rf dist",
    "build:gcc:es5": "java -jar ./node_modules/google-closure-compiler/compiler.jar --assume_function_wrapper --isolation_mode IIFE --use_types_for_optimization --language_in ECMASCRIPT_NEXT --language_out ECMASCRIPT5 --generate_exports --export_local_property_definitions --compilation_level ADVANCED --js_output_file=dist/superfine.es5.js 'src/**.js'",
    "build:gcc:es6": "java -jar ./node_modules/google-closure-compiler/compiler.jar --assume_function_wrapper --isolation_mode IIFE --use_types_for_optimization --language_in ECMASCRIPT_NEXT --language_out ECMASCRIPT_2015 --generate_exports --export_local_property_definitions --compilation_level ADVANCED --js_output_file=dist/superfine.es6.js 'src/**.js'",
    "build:gzip": "gzip dist/* -k -f",
    "prepare": "npm run build",
    "release": "npm run build && npm test && git commit -am $npm_package_version && git tag $npm_package_version && git push && git push --tags && npm publish"
  },
  "babel": {
    "presets": "env"
  },
  "devDependencies": {
    "babel-env": "2.4.1",
    "jest": "^23.3.0",
    "google-closure-compiler": "^20181008.0.0"
  }
}
